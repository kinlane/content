---
layout: none
---

[
	{% for post in site.posts limit:25 %}{
    "title": "{{ post.title }}",
    "url": "http://apievangelist.com{{ post.url }}",
    "updated": "{{ post.date | date: "%Y-%m-%dT%H:%M:%SZ" }}",
    "image": "{{ post.image }}",
    "github_user": "{{ site.github_user }}",
    "github_repo": "{{ site.github_repo }}"
  }{% if forloop.last %}{% else %},{% endif %}{% endfor %}
]
